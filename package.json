  "contributors": [{
    "name": "Patrik Laszlo",
    "email": "alabard@gmail.com",
    "url" : "http://patrikx3.com/"
  }],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrikx3/js-written-number"
  },
  "devDependencies": {
    "blanket": "1.1.7",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-uglify": "^2.3.0",
    "mocha": "^3.2.0",
    "mocha-spec-cov-alt": "^1.1.1",
    "mversion": "^1.10.0",
    "should": "^11.2.1"
  },
  "config": {
    "blanket": {
